extends layout
block content
      style.
        table { width: 100%; }
        th, tr, td {
          border: 1px solid black;
          padding: 10px;
          margin: 10px;
        }
      .wrapper
            h1 PMM Members
                a(href='/logout') Logout
            table
                for member in member_list
                    tr
                          th First Name
                          th Last Name
                          th Email
                          th Phone Number
                          th City
                          th State
                          th Crab Year
                    tr
                          td 
                              a(href=`http://localhost:3000/api/admin/user/${member.id}`)= member.firstName
                          td 
                              a(href=`http://localhost:3000/api/admin/user/${member.id}`)= member.lastName
                          td= member.email
                          td= member.phoneNumber
                          td= member.city
                          td= member.state
                          td= member.crabYear